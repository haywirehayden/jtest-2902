pipeline{
    agent any
    stages{
        stage("make directory"){
            steps{
                sh "mkdir ~/jenkins-pipelines || true"
            }
        }
        stage("add some files"){
            steps{
                sh "touch ~/jenkins-pipelines/file.txt"
                sh "ls -al"
            }
        }
        stage("output result to console"){
            steps{
                bash output.sh
            }
        }
    }
}
